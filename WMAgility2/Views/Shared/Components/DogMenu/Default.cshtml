@model IEnumerable<Dog>

<li class="dropdown">
    <a asp-controller="Dogs"
       asp-action="Index"
       class="dropdown" data-toggle="dropdown">Dogs<b class="caret"></b></a>
    <ul class="dropdown-menu">
        @foreach (var dog in Model)
        {
            <li>
                <a asp-controller="Dogs" asp-action="DogsList"
                   asp-route-id="@dog.Id">@dog.DogName</a>
            </li>
        }
        <li class="divider"></li>
        <li>
            <a asp-controller="Dogs" asp-action="Index" asp-route-Category="">All Dogs</a>
        </li>
    </ul>
</li>
