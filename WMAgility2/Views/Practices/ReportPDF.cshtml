@{
    Layout = null;
}

<html>
<head>
    <meta charset="utf-8">
    <title>Report</title>
    <link rel="stylesheet" href="style.css">
    <link rel="license" href="https://www.opensource.org/licenses/mit-license/">
    <script src="script.js"></script>
    <link href="~/css/demo.css" rel="stylesheet" />

</head>
<body>
    <header>
        <h1>Agility Report</h1>
        <address contenteditable>
            <p>WaggyMamas Agility School</p>
            <p>Beechhill Allotments</p>
            <p>The Scalp</p>
            <p>Wicklow</p>
        </address>
        <span><img src="wwwroot\images\wm_logo.png" /><input type="file" accept="image/*"></span>
    </header>
    <article>
        <h1>Student</h1>
        <address contenteditable>
            <p>Dog Name:</p>
        </address>
        <table class="meta">
            <tr>
                <th><span contenteditable>Report #</span></th>
                <td><span contenteditable>000001</span></td>
            </tr>
            <tr>
                <th><span contenteditable>Date</span></th>
                <td><span contenteditable>1 December 2020</span></td>
            </tr>
            <tr>
                <th><span contenteditable>Overall Success Rate: </span></th>
                <td><span id="prefix" contenteditable>र </span><span>Percentage</span></td>
            </tr>
        </table>
        <table class="inventory">
            <thead>
                <tr>
                    <th><span contenteditable>Skill: </span></th>
                    <th><span contenteditable>Dates:</span></th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><a class="cut">-</a><span contenteditable>Weaves</span></td>
                    <td><span contenteditable>July 2020</span></td>
                </tr>
            </tbody>
        </table>
    </article>
</body>
</html>  